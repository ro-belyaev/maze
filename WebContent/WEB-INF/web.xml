<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>jQuery_Ajax_Request</display-name>
  <servlet>
    <display-name>Registration</display-name>
    <servlet-name>RegisterUser</servlet-name>
    <servlet-class>ru.netcracker.belyaev.servlets.RegisterUser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MakeAction</servlet-name>
  	<servlet-class>ru.netcracker.belyaev.servlets.MakeAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterUser</servlet-name>
    <url-pattern>/RegisterUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MakeAction</servlet-name>
  	<url-pattern>/MakeAction</url-pattern>
  </servlet-mapping>
</web-app>